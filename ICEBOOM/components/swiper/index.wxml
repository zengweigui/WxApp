<swiper class='swiper' indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="height:{{imgheights[current]}}rpx;">
  <block wx:for="{{imgUrls}}" wx:key="{{index}}">
    <swiper-item class="swiper-item">
      <image src="{{item}}" class="slide-image" data-list="{{imgUrls}}" data-src="{{item}}" mode="widthFix" bindtap="preview" bindload="imageLoad"/>
    </swiper-item>
  </block>
</swiper>
